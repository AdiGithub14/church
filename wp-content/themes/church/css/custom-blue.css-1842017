/* style css */

@import url('https://fonts.googleapis.com/css?family=Carrois+Gothic|Pontano+Sans');
@import url('https://fonts.googleapis.com/css?family=Roboto');


* {
	margin: 0px;
	padding: 0px;
}
body {
	background: #FFF;
	height: 100%;
	width: 100%;
	font-weight: normal;
	margin: 0px;
	padding: 0px;
	font-size: 14px;
	font-family: 'Roboto', sans-serif;
	-webkit-font-smoothing: antialiased;
	font-smoothing: antialiased;
}
/*=======================================================

			WRAPPER

========================================================*/

#wrap {
	position: relative;
	width: 100%;
	overflow: hidden;
	background: #fff;
}
/*=======================================================

			HEADINGS

========================================================*/

h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
	color: #232323;
	-webkit-font-smoothing: antialiased;
}
h1 {
	font-size: 40px;
	font-family: 'Carrois Gothic', sans-serif;
}
h2 {
	font-size: 30px;
	font-family: 'Carrois Gothic', sans-serif;
}
h3 {
	font-size: 24px;
	font-family: 'Carrois Gothic', sans-serif;
}
h4 {
	font-size: 20px;
	/*font-weight: bold;*/
}
h5 {
	font-size: 18px;
}
h6 {
	font-size: 14px;
}
p {
	color: #777777;
	font-family: 'Roboto', sans-serif;
	font-size: 14px;
    line-height: 30px;
}

/* ========================== 
================= common
================================*/
div, table, tr, th, td, th, video, audio, h1, h2, h3, h4, h5, h6, aside, canvas, section, header, footer, nav, span, p, small, big, a, b, strong, img, hr, pre, button, input, textarea, optgroup, ul, li, ol, select, option, blockquote, nav {
	margin: 0;
	padding: 0;
	outline: none;
	border: none;
	text-decoration: none;
	list-style-type: none;
}
.clearfix:after {
	content: "";
	display: block;
	height: 0;
	clear: both;
	font-size: 0;
}
.clear {
	clear: both
}
* {
   font-smoothing: antialiased;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  -webkit-tap-highlight-color: transparent; 
}
img {
	max-width: 100%;
}
h1, h2, h3, h4, h5, h6 {
	-webkit-font-smoothing: antialiased;
}
input, textarea, select {
	width: 100%;
	height: 47px;
	margin: 0.125rem 0;
	border-radius: 0;
	text-indent: 5px;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	color: #575555;
	font-size: 15px;
	background: rgba(255,255,255,1);
	border: 1px solid #e5e4e4;
}
input:focus, textarea:focus, select:focus {
	background: rgba(255,255,255,0.93);
}
input[type="submit"] {
	width: 100%;
	background: transparent;
	color: #ffffff;
	text-align: center;
	line-height: 45px;
	font-size: 18px;
	cursor: pointer;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
}
input[type="submit"]:hover {
	background: #FFF;
	color: #f8db3a;
}
	
::-moz-placeholder {
 color:#656565;
}
::-ms-placeholder {
 color:#656565
}
::-webkit-placeholder {
 color:#656565
}
::-0-placeholder {
 color:#656565
}
::selection {
	background: #19a8e3;
	color: #fff;
}
::-moz-selection {
 background:#19a8e3;
 color:#fff;
}
::-webkit-selection {
 background:#19a8e3;
 color:#fff;
}
::-o-selection {
 background:#19a8e3;
 color:#fff;
}
::-ms-selection {
 background:#19a8e3;
 color:#fff;
}
.f_left {
	float: left;
}
.f_right {
	float: right;
}

strong, b {
    font-weight: 500;
    font-size: 17px;
}
a {
	-webkit-transition: all 0.4s ease-in-out;
	-moz-transition: all 0.4s ease-in-out;
	-o-transition: all 0.4s ease-in-out;
	-ms-transition: all 0.4s ease-in-out;
	transition: all 0.4s ease-in-out;
	text-decoration: none !important;
}
a:hover, a:focus {
	outline: none;
}
img {
	-webkit-transition: 0.4s ease-in-out;
	-moz-transition: 0.4s ease-in-out;
	-ms-transition: 0.4s ease-in-out;
	-o-transition: 0.4s ease-in-out;
	transition: 0.4s ease-in-out;
	display: inline-block !important;
}
li {
	font-family: 'Roboto', sans-serif;
	list-style: none;
}
a {
	font-family: 'Roboto', sans-serif;
	text-decoration: none;
	color: #757575;
}
.font-montserrat {
	font-family: 'Montserrat', sans-serif !important;
}
.font-lora {
	font-family: 'Carrois Gothic', sans-serif;
}
/*=======================================================

			SECTION

========================================================*/

section {
	position: relative;
	/*overflow: hidden;*/

	width: 100%;
}
.box-wide {
	width: 1370px !important;
	margin: 0 auto !important;
	overflow: hidden;
	box-shadow: 0px 0px 15px rgba(0,0,0,0.2);
}
.patern {
	background: url(../images/testi-bg.jpg) repeat;
}
/*=======================================================

			BUTTON

========================================================*/

.btn {
	background: #19a8e3;
	text-transform: uppercase;
	color: #232323;
	border-radius: 0px;
	display: inline-block;
	padding: 10px 18px;
	margin: 0 auto;
	margin-top: 15px;
	/*font-weight: bold;*/
	font-family: 'Roboto', sans-serif;
	-webkit-transition: all 0.4s ease-in-out;
	-moz-transition: all 0.4s ease-in-out;
	-o-transition: all 0.4s ease-in-out;
	-ms-transition: all 0.4s ease-in-out;
	transition: all 0.4s ease-in-out;
}
.btn:hover {
	background: #303030;
	color: #fff;
}
.black-btn {
    background: #222;
    padding: 5px 20px;
    margin-top: 35px;
    /*font-weight: bold;*/
    font-family: 'Carrois Gothic', sans-serif;
    font-size: 14px;
    color: #fff;
    display: inline-block;
    border: 2px #222 solid;
    line-height: 27px;
    text-align: center;
}
.black-btn:hover {
	background: none;
	border: 2px #19a8e3 solid;
	color: #19a8e3;
}
.black-btn:focus {
	background: none;
	border: 2px #19a8e3 solid;
	color: #19a8e3;
}
/*.black-btn i {
	padding-right: 8px;
}*/

hr {
	background: #19a8e3;
	height: 4px;
	width: 110px;
}
.tittle {
	text-align: center;
	display: inline-block;
	width: 100%;
	position: relative;
	margin-bottom: 60px;
}
.tittle h2 {
	font-family: 'Roboto', sans-serif;
	margin: 0px;
	font-weight: 200;
	line-height: 37px;
	color: #222222;
	margin-top: 5px;
	margin-bottom: 10px;
}
.tittle h2 span {
	/*font-weight: bold;*/
}
.tittle p {
	width: 75%;
	margin: 0 auto;
	line-height: 24px;
	font-size: 16px;
}
.underline {
	background: url(../images/span-underline.png) center 20px no-repeat;
	padding-bottom: 10px;
}
